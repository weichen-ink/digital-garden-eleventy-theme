{
  "trailingSlash": false,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*\\\\.(css|js))",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/content/(.*\\\\.(png|jpg|jpeg|gif|svg|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\\\.(woff|woff2|ttf|otf))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}