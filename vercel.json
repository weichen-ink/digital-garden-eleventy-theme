{
  "trailingSlash": false,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*\\.(css|js))",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/assets/(.*\\.(png|jpg|jpeg|gif|svg|webp|woff|woff2))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}